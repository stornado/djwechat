[
  {% for image in images %}{
    "oid": "{{ image.uuid }}",
    "name": "{{ image.title }}",
    "tags": [
      {% for tag in image.tags.all %}"{{tag}}"{% if not forloop.last %},{% endif %}
    {% empty %}{% endfor %}],
    "pubDate": "{{ image.publishedAt }}"
  }{% if not forloop.last %},{% endif %}
{% empty %}{% endfor %}]